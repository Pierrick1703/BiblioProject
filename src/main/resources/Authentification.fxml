        <TextField fx:id="usernameField" layoutX="226.0" layoutY="124.0" />
        <Label layoutX="226.0" layoutY="107.0" text="Nom d'utilisateur" />
        <Label layoutX="226.0" layoutY="183.0" text="Mot de Passe" />
        <Button fx:id="validateButton" layoutX="173.0" layoutY="260.0" mnemonicParsing="false" onAction="#validaton" text="Valider" />
