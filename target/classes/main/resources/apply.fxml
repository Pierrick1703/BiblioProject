<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.controllers.Apply">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="Fichier">
                    <items>
                        <MenuItem fx:id="MenuFichierOuvrir" mnemonicParsing="false" onAction="#fichierouvrir" text="Ouvrir" />
                        <MenuItem mnemonicParsing="false" text="Export PDF" onAction="#extractPDF"/>
                        <MenuItem fx:id="closeButton" mnemonicParsing="false" onAction="#closeButtonAction" text="Quitter" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edition">
                    <items>
                        <MenuItem fx:id="MenuEditionSauvegarder" mnemonicParsing="false" onAction="#editionsauvegarder" text="Sauvegarder sous..." />
                        <MenuItem fx:id="MenuEditionSauvegarderSous" mnemonicParsing="false" onAction="#editionsauvegardersous" text="Sauvegarder sous XML Locale" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="About">
                    <items>
                        <MenuItem fx:id="MenuAboutInfos" mnemonicParsing="false" onAction="#aboutinfos" text="Infos" />
                    </items>
                </Menu>
            <Menu mnemonicParsing="false" text="Connexion">
              <items>
                <MenuItem mnemonicParsing="false" text="Connexion BDD" />
              </items>
            </Menu>
            </menus>
        </MenuBar>
        <SplitPane dividerPositions="0.6158129175946548" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <items>
                        <TableView fx:id="TableView" editable="true" layoutX="-8.0" layoutY="14.0" prefHeight="480.0" prefWidth="550.0">
                            <columns>
                                <TableColumn fx:id="titre" prefWidth="75.0" text="Titre" />
                                <TableColumn fx:id="auteur" prefWidth="75.0" text="Auteur" />
                                <TableColumn fx:id="presentation" prefWidth="75.0" text="Présentation" />
                                <TableColumn fx:id="parution" prefWidth="56.0" text="Parution" />
                                <TableColumn fx:id="colonne" prefWidth="54.0" text="Colonne" />
                                <TableColumn fx:id="rangee" prefWidth="53.0" text="Rangée" />
                                <TableColumn fx:id="etat" prefWidth="68.0" text="État" />
                                <TableColumn fx:id="resumer" prefWidth="94.0" text="Resumé" />
                            </columns>
                        </TableView>
            <AnchorPane prefHeight="551.0" prefWidth="405.0">
               <children>
                  <ComboBox fx:id="etatField" layoutX="183.0" layoutY="174.0" prefHeight="25.0" prefWidth="101.0" visibleRowCount="2" />
                  <TextField fx:id="TitreField" layoutX="29.0" layoutY="73.0" prefHeight="25.0" prefWidth="258.0" />
                  <TextField fx:id="AuteurField" layoutX="29.0" layoutY="121.0" prefHeight="25.0" prefWidth="258.0" />
                  <TextField fx:id="PresentationField" layoutX="25.0" layoutY="377.0" prefHeight="25.0" prefWidth="230.0" />
                  <TextField fx:id="ColonneField" layoutX="25.0" layoutY="225.0" prefHeight="25.0" prefWidth="101.0" />
                  <TextField fx:id="RangeeField" layoutX="184.0" layoutY="225.0" prefHeight="25.0" prefWidth="101.0" />
                  <TextField fx:id="resumeeField" layoutX="25.0" layoutY="284.0" prefHeight="62.0" prefWidth="258.0" />
                   <TextField fx:id="ParutionField" layoutX="25.0" layoutY="174.0" prefHeight="25.0" prefWidth="101.0" />
                  <Label alignment="CENTER" layoutX="143.0" layoutY="14.0" prefHeight="36.0" prefWidth="86.0" text="Fiche Livre" textAlignment="CENTER" />
                  <Label layoutX="29.0" layoutY="56.0" text="Titre" />
                  <Label layoutX="29.0" layoutY="104.0" text="Auteur" />
                  <Label layoutX="32.0" layoutY="360.0" text="Présentation" />
                  <Label layoutX="25.0" layoutY="208.0" text="Colonne" />
                  <Label layoutX="186.0" layoutY="208.0" text="Rangée" />
                  <Label layoutX="185.0" layoutY="157.0" text="État" />
                  <Label layoutX="27.0" layoutY="266.0" text="Résumé" />
                  <Button fx:id="btnValider" layoutX="267.0" layoutY="517.0" mnemonicParsing="false" text="Ajouter" />
                  <Button fx:id="btnEdit" layoutX="261.0" layoutY="484.0" mnemonicParsing="false" onAction="#handleModifLivre" prefHeight="25.0" prefWidth="67.0" text="Modifier" />
                  <Button fx:id="btnSuppr" layoutX="259.0" layoutY="450.0" mnemonicParsing="false" onAction="#handleDeleteLivre" text="Supprimer" />
                  <Label layoutX="25.0" layoutY="157.0" text="Parution" />
                  <ImageView fx:id="imageViewParution" fitHeight="150.0" fitWidth="200.0" layoutX="26.0" layoutY="409.0" pickOnBounds="true" preserveRatio="true" />
                  <Button fx:id="buttonVerificationParution" layoutX="269.0" layoutY="377.0" mnemonicParsing="false" onAction="#verificationParution" text="Vérifier" />
               </children>
            </AnchorPane>
            </items>
        </SplitPane>
        <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0">
            <children>
                <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
                    <font>
                        <Font size="11.0" fx:id="x3" />
                    </font>
                    <textFill>
                        <Color red="0.625" green="0.625" blue="0.625" fx:id="x4" />
                    </textFill>
                </Label>
                <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
                <Label font="$x3" maxWidth="-1.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
            </children>
            <padding>
                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
            </padding>
        </HBox>
    </children>
</VBox>
